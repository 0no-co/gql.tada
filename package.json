{
  "name": "gql.tada",
  "description": "The spec-compliant & magical GraphQL query language engine in the TypeScript type system",
  "version": "0.0.0",
  "author": "0no.co <hi@0no.co>",
  "source": "./src/index.ts",
  "main": "./dist/gql-tada",
  "module": "./dist/gql-tada.mjs",
  "types": "./dist/gql-tada.d.ts",
  "sideEffects": false,
  "files": [
    "LICENSE.md",
    "README.md",
    "dist/"
  ],
  "exports": {
    ".": {
      "types": "./dist/gql-tada.d.ts",
      "import": "./dist/gql-tada.mjs",
      "require": "./dist/gql-tada.js",
      "source": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@0no-co/graphql.web": "^1.0.4"
  },
  "public": true,
  "keywords": [
    "graphql",
    "graphql-js",
    "client-side graphql"
  ],
  "scripts": {
    "test": "vitest test",
    "bench": "vitest bench --typecheck.enabled=false",
    "check": "tsc",
    "lint": "eslint --ext=js,ts .",
    "build": "rollup -c scripts/rollup.config.mjs",
    "clean": "rimraf dist node_modules/.cache",
    "prepublishOnly": "run-s clean build check test",
    "prepare": "node ./scripts/prepare.js",
    "changeset:version": "changeset version && pnpm install --lockfile-only",
    "changeset:publish": "changeset publish"
  },
  "repository": "https://github.com/0no-co/gql.tada",
  "bugs": {
    "url": "https://github.com/0no-co/gql.tada/issues"
  },
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 100,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint -c scripts/eslint-preset.js --fix",
    "*.json": "prettier --write",
    "*.md": "prettier --write"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "./scripts/eslint-preset.js"
    ]
  },
  "devDependencies": {
    "@0no-co/typescript.js": "5.3.2-2",
    "@babel/plugin-transform-block-scoping": "^7.23.4",
    "@babel/plugin-transform-typescript": "^7.23.6",
    "@changesets/cli": "^2.26.2",
    "@changesets/get-github-info": "^0.5.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/node": "^20.10.8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-tsdoc": "^0.2.17",
    "expect-type": "^0.17.3",
    "graphql": "^16.6.0",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^4.4.0",
    "rollup": "^3.19.1",
    "rollup-plugin-cjs-check": "^1.0.2",
    "rollup-plugin-dts": "^5.3.0",
    "terser": "^5.16.6",
    "typescript": "^5.3.3",
    "vitest": "1.0.4"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
